{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreDependencies": ["supports-color"],
  "ignoreFiles": [],
  "rules": {
    "binaries": "error",
    "dependencies": "error",
    "devDependencies": "error",
    "duplicates": "error",
    "exports": "warn",
    "files": "error",
    "types": "warn",
    "unlisted": "error",
    "unresolved": "error"
  },
  "workspaces": {
    ".": {
      "ignore": ["tsdown.config.ts"]
    },
    "packages/*": {
      "entry": "source/index.ts!",
      "project": ["source/**/*.ts!", "test/**/*.ts"]
    }
  }
}

{
  "name": "@arcanx/utilities",
  "description": "Shared utilities for arcanx packages",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": false,
  "author": {
    "email": "ini.dwiii@gmail.com",
    "name": "Dwi Payana",
    "url": "https://github.com/forgebyd"
  },
  "homepage": "https://github.com/forgebyd/arcanx/blob/main/packages/utilities",
  "readme": "https://github.com/forgebyd/arcanx/blob/main/packages/utilities/README.md",
  "repository": {
    "directory": "packages/utilities",
    "type": "git",
    "url": "git+https://github.com/forgebyd/arcanx.git"
  },
  "packageManager": "pnpm@10.23.0",
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "24.11.1",
    "pnpm": "10.23.0"
  },
  "engines": {
    "node": ">=22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "dev": "./source/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "keywords": [
    "forgebyd",
    "arcanx",
    "automation",
    "cli",
    "command",
    "command-line",
    "file-generator",
    "code-generator",
    "utilities",
    "typescript"
  ],
  "scripts": {
    "build": "pnpm run -w build --filter 'utilities'",
    "start.prod": "node ./dist/index.mjs",
    "start.dev": "node --conditions=dev ./dist/index.mjs",
    "dev": "tsx --conditions=dev ./source/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@arcanx/types": "workspace:*",
    "chalk": "^5.6.2",
    "change-case": "^5.4.4",
    "lodash.concat": "^4.5.0",
    "lodash.has": "^4.5.2"
  },
  "devDependencies": {
    "@types/lodash.concat": "^4.5.9",
    "@types/lodash.has": "^4.5.9"
  }
}
